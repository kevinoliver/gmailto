{
    "manifest_version": 3,
    "default_locale": "en",

    "name": "gmailto",
    "description": "A Safari extension that makes mailto: links use GMail in your browser",
    "version": "1.0",

    "icons": {
        "48": "images/icon-48.png",
        "96": "images/icon-96.png"
    },

    "content_scripts": [{
        "js": [ "content.js" ],
        "matches": [ "https://*/*" ]
    }],

    "permissions": [ ]
}
